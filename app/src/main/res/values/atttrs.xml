<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TypefacedTextView">
        <attr name="typeface" format="string" />
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer" />
        <attr name="stroke_width" format="dimension" />
        <attr name="color_tick" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="color_unchecked" format="color" />
        <attr name="color_unchecked_stroke" format="color" />
    </declare-styleable>

    <declare-styleable name="InvertedTextProgressbar">
        <attr name="text" format="string"/>
        <attr name="text_color" format="color"/>
        <attr name="text_inverted_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_typeface" format="integer"/>
        <attr name="max_progress" format="integer"/>
        <attr name="min_progress" format="integer"/>
    </declare-styleable>


</resources>